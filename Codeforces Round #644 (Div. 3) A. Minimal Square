//problem:https://codeforces.com/contest/1360/problem/A
    #include <bits/stdc++.h>
    using namespace std;
     
    int main() {
    	int t;
    	cin>>t;
    	while(t--)
    	{
    	    int a,b,n=0;
    	    cin>>a>>b;
    	    if(a<b)
    	    swap(a,b);
    	    n= max(a,2*b);
    	    cout<<n*n<<endl;
    	}
    }
